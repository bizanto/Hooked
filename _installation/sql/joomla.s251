CREATE TABLE `#__contact_details` (   `id` int(11) NOT NULL AUTO_INCREMENT,   `name` varchar(255) NOT NULL DEFAULT '',   `alias` varchar(255) NOT NULL DEFAULT '',   `con_position` varchar(255) DEFAULT NULL,   `address` text,   `suburb` varchar(100) DEFAULT NULL,   `state` varchar(100) DEFAULT NULL,   `country` varchar(100) DEFAULT NULL,   `postcode` varchar(100) DEFAULT NULL,   `telephone` varchar(255) DEFAULT NULL,   `fax` varchar(255) DEFAULT NULL,   `misc` mediumtext,   `image` varchar(255) DEFAULT NULL,   `imagepos` varchar(20) DEFAULT NULL,   `email_to` varchar(255) DEFAULT NULL,   `default_con` tinyint(1) unsigned NOT NULL DEFAULT '0',   `published` tinyint(1) unsigned NOT NULL DEFAULT '0',   `checked_out` int(11) unsigned NOT NULL DEFAULT '0',   `checked_out_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',   `ordering` int(11) NOT NULL DEFAULT '0',   `params` text NOT NULL,   `user_id` int(11) NOT NULL DEFAULT '0',   `catid` int(11) NOT NULL DEFAULT '0',   `access` tinyint(3) unsigned NOT NULL DEFAULT '0',   `mobile` varchar(255) NOT NULL DEFAULT '',   `webpage` varchar(255) NOT NULL DEFAULT '',   PRIMARY KEY (`id`),   KEY `catid` (`catid`) ) ENGINE=MyISAM DEFAULT CHARSET=utf8;
CREATE TABLE `#__content` (   `id` int(11) unsigned NOT NULL AUTO_INCREMENT,   `title` varchar(255) NOT NULL DEFAULT '',   `alias` varchar(255) NOT NULL DEFAULT '',   `title_alias` varchar(255) NOT NULL DEFAULT '',   `introtext` mediumtext NOT NULL,   `fulltext` mediumtext NOT NULL,   `state` tinyint(3) NOT NULL DEFAULT '0',   `sectionid` int(11) unsigned NOT NULL DEFAULT '0',   `mask` int(11) unsigned NOT NULL DEFAULT '0',   `catid` int(11) unsigned NOT NULL DEFAULT '0',   `created` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',   `created_by` int(11) unsigned NOT NULL DEFAULT '0',   `created_by_alias` varchar(255) NOT NULL DEFAULT '',   `modified` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',   `modified_by` int(11) unsigned NOT NULL DEFAULT '0',   `checked_out` int(11) unsigned NOT NULL DEFAULT '0',   `checked_out_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',   `publish_up` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',   `publish_down` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',   `images` text NOT NULL,   `urls` text NOT NULL,   `attribs` text NOT NULL,   `version` int(11) unsigned NOT NULL DEFAULT '1',   `parentid` int(11) unsigned NOT NULL DEFAULT '0',   `ordering` int(11) NOT NULL DEFAULT '0',   `metakey` text NOT NULL,   `metadesc` text NOT NULL,   `access` int(11) unsigned NOT NULL DEFAULT '0',   `hits` int(11) unsigned NOT NULL DEFAULT '0',   `metadata` text NOT NULL,   PRIMARY KEY (`id`),   KEY `idx_section` (`sectionid`),   KEY `idx_access` (`access`),   KEY `idx_checkout` (`checked_out`),   KEY `idx_state` (`state`),   KEY `idx_catid` (`catid`),   KEY `idx_createdby` (`created_by`),   KEY `listing_count` (`sectionid`,`catid`,`state`,`access`,`publish_up`,`publish_down`),   KEY `published` (`state`,`catid`),   KEY `created` (`created`) ) ENGINE=MyISAM AUTO_INCREMENT=60397 DEFAULT CHARSET=utf8;
INSERT INTO `#__content` VALUES ('374', 'Gjeddegrunna', 'gjeddegrunna', '', '<p>Navnet sier det meste - ca 50 meter fra land.</p> <p> </p> <p><a href=\"http://www.ofa.no/page?id=1507&key=30599\" target=\"_blank\">Kilde: Marjo fra OFA forumet</a></p>', '', '1', '1', '0', '4', '2011-02-17 07:36:00', '148', '', '2011-04-28 21:07:00', '64', '0', '0000-00-00 00:00:00', '2011-02-17 07:36:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '398', '', '', '0', '92', ''), ('375', 'Dorging', 'dorging', '', '<p>Å dorge betyr å fiske med redskap etter en båt/kano, gjerne i varierende fart for å fiske på ulike dybder.</p> <p>Du kan dorge med mange ulike agn & kroker. Eksempelvis;</p> <p>fiskeharpe, sluk, wobler, markdrag eller andre redskap</p> <p> </p> <p><em>Trolling</em> er også i all praksis å dorge, men med mer \"moderne\" utstyr, flere stenger, dyprigger etc.</p>', '', '1', '14', '0', '24', '2011-02-17 07:38:00', '103', '', '2011-04-26 09:00:00', '103', '0', '0000-00-00 00:00:00', '2011-02-17 07:38:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '2', 'Dorge', 'Å dorge betyr å fiske med redskap etter en båt/kano, gjerne i varierende fart for å fiske på ulike dybder.\nDu kan dorge med mange ulike agn & kroker. Eksempelvis;\nfiskeharpe, sluk, wobler, markdrag eller andre redskap', '0', '755', ''), ('376', 'Bremsrud', 'bremsrud', '', '<p>Røret fra Elvåga. Her finner du abbor og gjedde</p> <p> </p> <p><a href=\"http://www.ofa.no/page?id=1507&key=30599\" target=\"_blank\">Kilde: Marjo fra OFA forumet</a></p>', '', '1', '1', '0', '4', '2011-02-17 07:43:00', '148', '', '2011-04-28 21:07:00', '64', '0', '0000-00-00 00:00:00', '2011-02-17 07:43:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '419', '', '', '0', '110', ''), ('377', 'OFA plassen', 'ofa-plassen', '', '<p>Rett ovenfor Lakeplassen kommer det opp et skjær ca 20 meter fra land. Ut her er en grunne på ca 4-5 meter.</p> <p>God plass for ørret og gjedde</p> <p> </p> <p><a href=\"http://www.ofa.no/page?id=1507&key=30599\" target=\"_blank\">Kilde: Marjo fra OFA forumet</a></p>', '', '1', '1', '0', '4', '2011-02-17 08:01:00', '148', '', '2011-04-28 21:07:00', '64', '0', '0000-00-00 00:00:00', '2011-02-17 08:01:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '420', '', '', '0', '68', ''), ('324', 'Hemsil / Hemsila', 'hemsil', '', '<h2>Hemsila / Hemsil er en fluefiskeelv i Hemsedal</h2> <p>Årlig settes ut 3000 3-somrig ørret i Hemsil-vassdraget, ingen utsetting i fluesona.</p> <p>Fluesona har svært god gyting, og denne delen av elva er selvrekrutterende. 2-hvert år tas stamfiske med garn. Av en omsetning på nærmere kr. 1.000.000 for året 2006, brukes kr 500 000 til oppsyn, leie av fiskevann, utsetting av fisk, skilting, rydding/merking av stier, info/foldere osv.</p> <p>En ørret på 500 gram er 6-7 år gammel. Fra denne alder vokser den ca. 200 gram pr. år. Snittvekt på ørret i Hemsil er fra 3-900 gram, men det er også en stor bestand av ørret fra 900-1500 gram. Ørreten i Hemsil er fiskespiser, og derfor er denne med på å regulere bestanden (viktig med stor fisk i  bestanden).</p> <p>Fiskereglene i fluesona er nødvendig for å ha en god fiskeelv.</p>', '', '1', '1', '0', '2', '2011-02-13 16:59:00', '103', '', '2011-12-15 11:41:00', '103', '0', '0000-00-00 00:00:00', '2011-02-13 16:59:00', '0000-00-00 00:00:00', 'jreviews/_Hemsila_1297616379.JPG|||0||bottom||', '', '', '1', '0', '0', '', '', '0', '3430', ''), ('333', 'Langesethølen', 'langesetholen', '', '', '', '1', '1', '0', '4', '2011-02-13 19:04:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-02-13 19:04:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '421', '', '', '0', '70', ''), ('334', 'Mythelåttan', 'mythelattan', '', '', '', '1', '1', '0', '4', '2011-02-13 19:06:00', '103', '', '2011-02-19 10:58:00', '64', '0', '0000-00-00 00:00:00', '2011-02-13 19:06:00', '0000-00-00 00:00:00', 'jreviews/334_P6290046_1298113097.JPG|||0||bottom||', '', '', '1', '0', '422', '', '', '0', '88', ''), ('335', 'Telegrafen', 'telegrafen', '', '<p>Bra strekke</p>', '', '1', '1', '0', '4', '2011-02-13 19:10:00', '103', '', '2011-02-19 10:07:00', '64', '0', '0000-00-00 00:00:00', '2011-02-13 19:10:00', '0000-00-00 00:00:00', 'jreviews/335_Telegrafen_1298110075.jpg|||0||bottom||', '', '', '1', '0', '423', '', '', '0', '82', ''), ('336', 'Styrmannsøyene', 'styrmannsoyene', '', '', '', '1', '1', '0', '4', '2011-02-13 21:58:00', '103', '', '2011-02-19 10:02:00', '64', '0', '0000-00-00 00:00:00', '2011-02-13 21:58:00', '0000-00-00 00:00:00', 'jreviews/336_Styrmannsyene_1298109776.jpg|||0||bottom||', '', '', '1', '0', '424', '', '', '0', '75', ''), ('337', 'Krikken', 'krikken', '', '', '', '1', '1', '0', '4', '2011-02-13 22:01:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-02-13 22:01:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '425', '', '', '0', '87', ''), ('338', 'Solstad', 'solstad', '', '<p>En liten hengebru over et lite \'stryk\' med bra sømmer og litt dybde</p>', '', '1', '1', '0', '4', '2011-02-13 22:09:00', '103', '', '2011-02-19 09:57:00', '64', '0', '0000-00-00 00:00:00', '2011-02-13 22:09:00', '0000-00-00 00:00:00', 'jreviews/338_Solstadbru_1298109479.jpg|||0||bottom||', '', '', '1', '0', '426', '', '', '0', '64', ''), ('371', 'Parkering', 'parkering', '', '', '', '1', '2', '0', '6', '2011-02-15 12:58:00', '64', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-02-15 12:58:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '0', '', '', '0', '23', ''), ('325', 'Tuv', 'tuv', '', '', '', '1', '1', '0', '4', '2011-02-13 17:07:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-02-13 17:07:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '427', '', '', '0', '36', ''), ('326', 'Nilstad', 'nilstad', '', '<p>Fint strekke det her!</p>', '', '1', '1', '0', '4', '2011-02-13 17:12:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-02-13 17:12:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '428', '', '', '0', '42', ''), ('327', 'Ørjanhaugen', 'orjanhaugen', '', '', '', '1', '1', '0', '4', '2011-02-13 18:22:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-02-13 18:22:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '429', '', '', '0', '48', ''), ('328', 'Sirkuset', 'sirkuset', '', '<p>En stillegående og krevende del. Fryktelig morsomt!</p>', '', '1', '1', '0', '4', '2011-02-13 18:47:00', '103', '', '2011-02-23 19:08:00', '64', '0', '0000-00-00 00:00:00', '2011-02-13 18:47:00', '0000-00-00 00:00:00', 'jreviews/328_Sirkus2_1298110613.jpg|||0||bottom||', '', '', '1', '0', '430', '', '', '0', '346', ''), ('64', 'Gullbust', 'gullbust', '', '<p>Gullbusten har en mengde små svarte prikker i regnbuehinnen på øye. Et særpreg hos gullbusten er snuten, som har en avrundet og overhengende fasong. Halestilken er slank. Gullbusten har visse likhetstrekk med <a href=\"index.php?option=com_content&view=article&id=70&catid=17&Itemid=223\" target=\"_blank\">mort</a>, men i motsetning til denne har den gulaktige øyne (morten har røde).</p> <p>Gullbusten trives best i rennende vann, selv om den også forekommer i stillestående vann og innsjøer. Den er en typisk stimfisk som livnærer seg av insekter på overflaten og krepsdyr og planter på bunnen.</p> <p>I Norge er Gullbust utbredt i Glommavassdraget, Haldenvassdraget og Drammenselva. I tillegg enkelte bekker og vann på Østlandet.</p> <p> </p> <p>Gullbust er lite attraktiv som sportsfisk, noe som skyldes den beskjedne størrelsen. Sportsfiskerekorden er på 340 gram fra Glomma. I England bedrives det meite- og fluefiske etter den.</p> <p> </p> <p>Les mer på <a href=\"http://fiskipedia.no/Arter/gullbust.html\" target=\"_blank\">www.fiskipedia.no</a></p> <p> </p> <p> </p>', '', '1', '7', '0', '17', '2011-02-10 22:22:00', '103', '', '2011-04-17 08:24:00', '64', '0', '0000-00-00 00:00:00', '2011-02-10 22:22:00', '0000-00-00 00:00:00', 'jreviews/_Gullbust_1297376564.jpg|||0||bottom||', '', '', '1', '0', '72', 'Gullbust, mort', 'Gullbusten har en mengde små svarte prikker i regnbuehinnen på øye. Et særpreg hos gullbusten er snuten, som har en avrundet og overhengende fasong. Halestilken er slank. Gullbusten har visse likhetstrekk med mort, men i motsetning til denne har den gulaktige øyne (morten har røde).', '0', '541', ''), ('56271', 'Abbor (???? gr) @ Nitelva', 'abbor--gr--nitelva', '', 'en fin dag under brua', '', '1', '6', '0', '14', '2011-04-26 18:21:26', '133', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-04-26 18:21:26', '0000-00-00 00:00:00', '', '', '', '1', '0', '1', '', '', '0', '435', ''), ('55', 'Abbor', 'abbor', '', '<p><strong>Abbor er en ypperlig sportsfisk</strong> og spesielt i vinterhalvåret er den en meget populær fisk.</p> <p>På isfiskekonkurranser er det ofte bare abbor som er tellende art. Siden slutten av 80-tallet har det vært en gryende interesse i det å fange stor abbor, og mange sportsfiskeres gjeveste drøm er å komme over 2 kilos streken.</p> <p><strong>Abboren er en utpreget rovfisk</strong> som kan fiskes med de aller fleste redskaper, noen av de mest populære metodene er pimpel, mormyska og balansepilk på isen, mens sommerabboren ofte lar seg lure av sluk, wobbler, mark og agnfisk.</p> <p>Fargene går oftest i brun, grønn og svart, hvor den som regel har 5-7 langsgående svarte striper fra ryggen og ned mot buken. Bryst og gattfinne er ofte, og spesielt i skogsvann, farget knall oransje, og noen ganger nesten helt røde.</p> <p> </p> <p><strong>Hvordan fange abbor?</strong></p> <p><a href=\"index.php?option=com_content&view=article&id=59946&catid=136\">Les mer her</a></p> <p><strong><br /></strong></p> <p>Les mer på <a href=\"http://fiskipedia.no/Arter/abbor.html\" target=\"_blank\">www.fiskipedia.no</a></p>', '', '1', '7', '0', '17', '2011-02-10 15:37:00', '103', '', '2011-11-01 12:51:00', '103', '0', '0000-00-00 00:00:00', '2011-02-10 15:37:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '60', '', 'Fiske abbor', '0', '1232', ''), ('56', 'Asp', 'asp', '', '<p>Aspen er meget populær som sportsfisk pga sin størrelse og kampvilje. Aspen tar agnet utrolig hardt og har en bra fight helt til den er i håven.</p> <p>Pga sin begrensede utbredelse i Norge er den blitt oppført på rødlista, så det henstilles til at all asp blir satt uskadd og levende tilbake.</p> <p>Sportsfiskerekorden er på 5340 gram fra Øyeren.</p> <p><strong>Utseende</strong></p> <p>Lang kropp og hode. Oversiden med grønnskjær til svart, sidene sølv ofte med blåskjær. Rygg-, gatt- og halefinne er mørke brune til svarte. Tydelig underbitt med skarpe tenner. Gattfinnen er litt lengre enn ryggfinnen.</p> <p> </p> <p>I Norge kun utbredt i Øyernvassdraget inklusiv nedre deler av Glomma, Leira og Nitelva samt ett funn fra Skulerudsjøen i 2008.</p> <p>Lever ofte i de frie vannmasser, hvor den jakter byttefisk som laue, krøkle og annen småfisk. Utpreget rovfisk som foruten fisk, også tar til takke med fugleunger, frosker og mus. Gytingen foregår i de nedre delene av elvene i april-juni. Trivselstemperatur ved 4-20 grader.</p> <p> </p> <p>Les mer på <a href=\"http://fiskipedia.no/Arter/asp.html\" target=\"_blank\">www.fiskipedia.no</a></p>', '', '1', '7', '0', '17', '2011-02-10 15:38:00', '103', '', '2011-04-17 06:59:00', '64', '0', '0000-00-00 00:00:00', '2011-02-10 15:38:00', '0000-00-00 00:00:00', 'jreviews/56_Asp_1297374892.jpg|||0||bottom||', '', '', '1', '0', '59', '', '', '0', '174', ''), ('58', 'Bekkerøye', 'bekkeroye', '', '<p>Bekkerøya er mørk grønn til brun med marmorering i lysere farger på ryggen og ofte halen. Mage og finner på undersiden av fisker er ofte røde, og fisken har ofte røde prikker. Rødfargen på magen blir kraftigere i gytetiden.</p> <p>Den største registrerte lengden på bekkerøye er 86 cm, og den største vekta er 9,4 kg. Den kan nå en alder på minst 7 år, kanskje det dobbelte på steder den er satt ut.</p> <p>Lutvann ved Oslo er et kjent bekkerøyevann, men hovedområdet er nok i Njardheimsområdet i øvre Sirdal, der feks Rosskreppfjorden er et kjent vann med særdeles stor fisk.</p> <p> </p> <p>Les mer på <a href=\"http://no.wikipedia.org/wiki/Bekker%C3%B8ye\" target=\"_blank\">www.wikipedia.no</a></p>', '', '1', '7', '0', '17', '2011-02-10 15:39:00', '103', '', '2011-04-17 07:11:00', '64', '0', '0000-00-00 00:00:00', '2011-02-10 15:39:00', '0000-00-00 00:00:00', 'jreviews/58_Bekkerye_1297375346.jpg|||0||bottom||', '', '', '1', '0', '61', '', 'ekkerøya er mørk grønn til brun med marmorering i lysere farger på ryggen og ofte halen. Mage og finner på undersiden av fisker er ofte røde, og fisken har ofte røde prikker. Rødfargen på magen blir kraftigere i gytetiden. ', '0', '512', ''), ('59', 'Brasme', 'brasme', '', '<p>Brasme er en typisk lavlandsart som trives best i grunne innsjøer og elver med mye vegetasjon. Den trives best i innsjøer med mudderbunn, men kan påtreffes i alle typer innsjøer.</p> <p>I Norge er brasme kun utbredt på Østlandet fra Mjøsa i nord til Larvik og Halden i sør.</p> <p> </p> <p>Med sin høye sammentrykte kropp kan brasme kun forveksles med flire. Øynene hos brasme er vesentlig mindre enn hos flire og diameteren på øye er mindre enn avstanden fra øye til snute. Brasme har mørke finner, nesten svarte, mens flira har mer gjennomsiktige med rødlig skjær. Kroppsfargen hos brasme er sølvfarget på yngre individer, mens eldre brasmer har bronsje- til brun farge.</p> <p>Brasme har den egenskapen at den kan skyte fram munnen, dette bruker den når den leter etter mat i bunnsegmentet på kvelden og natta. Da skyter den munnen ut og suger opp mudder, for så å sile ut maten av dette.</p> <p>Brasmen er ikke kresen i matveien og spiser det meste den kommer over, alle typer bunndyr, rogn og småfisk står høyt på menyen.</p> <p> </p> <p>Gytingen foregår tidlig på våren, ofte ved at store stimer med brasme trekker opp i elvene, dette skjer gjerne samtidig som isen trekker seg vekk fra elvene. Selve gytingen foregår i mai-juni på meget grunt vann. Hannen har da fått sine karakteristiske gytevorter på hode og forkroppen. Hunnen gyter mellom 90 000 til 300 000 egg som måler 1,6 til 1,8 mm. Disse fester seg til vannplanter og klekkes etter 8-12 døgn.</p> <p> </p> <p>Brasmen kan bli opptil 9 kilo i Europa, mens det største kjente eksemplaret fra Norge ble fanget i Steinsfjorden og veide 7 kilo. Arten kan bli 82 cm lang og oppnå en alder av 23 år.</p> <p>Brasme er en utmerket sportsfisk, som helst fiskes på bunn med mark.</p> <p>Det største sportsfiskede eksemplaret fra Norge er på 5060 gram og er tatt i Steinsfjorden.</p> <p> </p> <p>Les mer på <a href=\"http://fiskipedia.no/Arter/brasme.html\" target=\"_blank\">www.fiskipedia.no</a></p>', '', '1', '7', '0', '17', '2011-02-10 15:39:00', '103', '', '2011-04-17 07:31:00', '64', '0', '0000-00-00 00:00:00', '2011-02-10 15:39:00', '0000-00-00 00:00:00', 'jreviews/59_Brasme_1297377969.jpg|||0||bottom||', '', '', '1', '0', '58', 'Brasme', 'Brasme er en typisk lavlandsart som trives best i grunne innsjøer og elver med mye vegetasjon. Den trives best i innsjøer med mudderbunn, men kan påtreffes i alle typer innsjøe', '0', '637', ''), ('63', 'Dvergmalle', 'dvergmalle', '', '<p>Dvergmalle er den eneste representanten av mallefisker i Norge og kan ikke forveksles med noen annen ferskvannsfisk.</p> <p>Dvergmalle har åtte kraftige og forholdsvis lange skjeggtråder rundt munnen. Den har 7-8 finnestråler i ryggfinnen hvorav den første strålen er en kraftig piggstråle.</p> <p>Dvergmalle er en typisk nattaktiv fisk, som finner føde på bunnen ved hjelp av de lange skjeggtrådene. Føden består for det meste av insektlarver, muslinger, snegler, rogn og småfisk.</p> <p>Den trives best på bløt bunn, og er temmelig hardfør. Den klarer seg godt selv i oksygenfattige, forurensede vann med temperaturer langt over 30C. Normalt finner man den fra 0 til 3 meters dyp, men den kan påtreffes ned til 40 meter.</p> <p>Norsk sportsfiskerekord ble tatt i 2005 med en fisk på 540 gram fra Asker.</p> <p> </p> <p>Les mer på <a href=\"http://fiskipedia.no/Arter/dvergmalle.html\" target=\"_blank\">www.fiskipedia.no</a></p>', '', '1', '7', '0', '17', '2011-02-10 22:11:00', '103', '', '2011-04-17 07:55:00', '64', '0', '0000-00-00 00:00:00', '2011-02-10 22:11:00', '0000-00-00 00:00:00', 'jreviews/_Dvergmalle_1297375879.jpg|||0||bottom||', '', '', '1', '0', '57', 'Dvergmalle, Malle, skjeggtråd', 'Dvergmalle har åtte kraftige og forholdsvis lange skjeggtråder rundt munnen. Den har 7-8 finnestråler i ryggfinnen hvorav den første strålen er en kraftig piggstråle.\nDvergmalle er den eneste representanten av mallefisker i Norge og kan ikke forveksles med noen annen ferskvannsfisk.', '0', '342', ''), ('61', 'Gjedde', 'gjedde', '', '<p>Gjedde er en langstrakt fisk med en flat og bred snute. Ryggfinnen og gattfinnen sitter langt bak på kroppen og er like lange. Hanngjedda oppnår sjelden en vekt på mer enn 8 kg, mens hunngjedda kan komme opp i mer enn 30 kg.</p> <p>Gjeddene holder ofte til på steder med mye vegetasjon som blekjer (vannliljeblader) og siv. Den er en glupsk rovfisk med skarpe tenner som spiser det meste den kommer over av andre fisker, frosker, vannrotter og mindre vadefugler. Gjedda er en svært hurtig fisk som kan gjøre lynraske utfall mot byttedyr. Den er derimot ikke en særlig utholdende svømmer og står derfor ofte rolig i vegetasjonen og venter på at et passende bytte skal komme forbi. Om vinteren trekker gjeddene ofte ut på dypere vann. Gytetiden er i periode april-mai.</p> <p>Norgesrekorden er en gjedde på 19,6 kg, tatt i garn ved Synneren på Ringerike.</p> <p>Den største gjedda som er fanget med stang ble tatt av 11 år gamle Tore Harry Halvorsen i et badevann ved Grimstad i 2006 og veide 18,7 kilo.</p> <p>Den største gjedda som noen gang er tatt ble fanget i garn av Arno Wilhelm i en innsjø nær Dannstadt i Tyskland i 1983 og veide hele 30,5 kilo. Kjempegjedda målte 146 cm.</p> <p>Verdensrekorden (IFGA) på stang er på ca. 24,8 kg og innehas av Lothar Louis. Rekordgjedda ble tatt i innsjøen Grefeern i Tyskland den 16. oktober i 1986.</p> <p> </p> <p>Les mer på <a href=\"http://no.wikipedia.org/wiki/Gjedde\" target=\"_blank\">www.wikipedia.no</a></p>', '', '1', '7', '0', '17', '2011-02-10 15:40:00', '103', '', '2011-04-17 08:08:00', '64', '0', '0000-00-00 00:00:00', '2011-02-10 15:40:00', '0000-00-00 00:00:00', 'jreviews/61_Gjedde_1297376159.jpg|||0||bottom||', '', '', '1', '0', '56', 'Gjedde', 'Gjedde er en langstrakt fisk med en flat og bred snute. Ryggfinnen og gattfinnen sitter langt bak på kroppen og er like lange. Hanngjedda oppnår sjelden en vekt på mer enn 8 kg, mens hunngjedda kan komme opp i mer enn 30 kg.', '0', '1273', ''), ('62', 'Gjørs', 'gjors', '', '<p>Gjørs er en fisk i abborfamilien. Den ligner på abboren, men er noe slankere og mer langstrakt. Den kan oppnå en lengde på hele 130 cm og en vekt på 18 kg. I Norge sjelden mer enn 12 kg. Gjørs regnes som en ypperlig matfisk og er hvit i kjøttet.</p> <p> </p> <p>I Norge finnes gjørsen kun i Tista, Glomma opp til Øyeren og sør i Leira, Vansjø , Gjersjøen , Akersvannet , Nærevannet , Isesjøen og Borrevannet. I Norge benyttes den i liten grad som matfisk, men den er blitt en populær sportsfisk.</p> <p> </p> <p>Les mer på <a href=\"http://no.wikipedia.org/wiki/Gj%C3%B8rs\" target=\"_blank\">www.wikipedia.no</a></p>', '', '1', '7', '0', '17', '2011-02-10 15:41:00', '103', '', '2011-04-17 08:11:00', '64', '0', '0000-00-00 00:00:00', '2011-02-10 15:41:00', '0000-00-00 00:00:00', 'jreviews/62_Gjrs_1297376353.jpg|||0||bottom||', '', '', '1', '0', '55', 'Gjørs', 'Gjørs er en fisk i abborfamilien. Den ligner på abboren, men er noe slankere og mer langstrakt. Den kan oppnå en lengde på hele 130 cm og en vekt på 18 kg. I Norge sjelden mer enn 12 kg. Gjørs regnes som en ypperlig matfisk og er hvit i kjøttet.', '0', '632', ''), ('4', 'Haspel fiske (Rod \'n Reel)', 'haspel-fiske', '', '&nbsp;', '', '-2', '14', '0', '24', '2010-11-18 08:09:00', '103', '', '2011-01-24 23:45:54', '62', '0', '0000-00-00 00:00:00', '2010-11-18 08:09:00', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '2', '0', '0', '', '', '0', '11', 'robots=\nauthor='), ('5', 'Isfiske (Freshwater Ice Fishing)', 'isfiske', '', '&nbsp;', '', '-2', '14', '0', '24', '2010-11-18 08:10:00', '103', '', '2011-01-24 23:45:21', '62', '0', '0000-00-00 00:00:00', '2010-11-18 08:10:00', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '2', '0', '0', '', '', '0', '13', 'robots=\nauthor='), ('6', 'Båtfiske', 'batfiske', '', '<p><strong>Båtfiske er veldig populært</strong> og kanskje den aller mest utbredte fiskemetoden i Norge i dag.</p>\r\n<p>Det er lett, rimelig og tilgjengelig for alle! (Men kan også gjøres avansert, dyrt og artig - man får det ikke morsommere enn man gjør det til selv)</p>\r\n<p> </p>\r\n<h2>Fjordfiske med båt</h2>\r\n<div><strong>Lett fjordfiske</strong> fra båt krever enkelt utstyr og du kommer langt med en fiskeharpe og litt gumimakk. Funker bra enten man er ankret opp eller drivende. Agn på med en fiskebit eller to, og sett igang. Enkle ekkolodd kan virkelig være gode å ha, og ikke vær redd for å være i bevegelse.</div>\r\n<p> </p>\r\n<p> </p>\r\n<h2>Havfiske med båt</h2>\r\n<p>Båtfiske etter de største artene skjer ofte langt til havs eller på dype steder, og da skal du være sikker på at båt og utstyr fungerer. Avhengig av type båt og størrelse og bruk, bør man evaluere bæreevne, lensemuligheter og nok ståplass til alle som vil. Motorstørrelse er også viktig, slik at du kommer både raskt ut til fiskeplassen, og like kjapt tilbake i tilfelle væromslag.</p>\r\n<p><strong>Utstyr til litt tyngre havfiske kan være</strong>;</p>\r\n<p> </p>\r\n<ul>\r\n<li>Stang 20-30 lbs</li>\r\n<li>Snelle 20 - 30 lbs</li>\r\n<li>Snøre - Multifilament</li>\r\n<li>Pilk</li>\r\n<li>Angfisk</li>\r\n<li>Klepp</li>\r\n<li>Svirvler, kroker, bly, fortom, gummimakk etc</li>\r\n<li>En kompis med stor fin båt, ekolodd, radar og egen spisehall. Insister gjerne på dusjmuligheter.</li>\r\n<li>Flytedress</li>\r\n<li>Varme sko</li>\r\n<li>Fiskesele for de store gutta</li>\r\n</ul>\r\n<p> </p>', '', '1', '14', '0', '48', '2010-11-18 08:10:00', '103', '', '2011-04-15 17:27:21', '64', '0', '0000-00-00 00:00:00', '2010-11-18 08:10:00', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '10', '0', '3', '', '', '0', '978', 'robots=\nauthor='), ('7', 'Isfiske', 'isfiske-saltvann', '', '<h2><strong>Isfiske på sjøen er gøy, men vær aktsom på sjøisen</strong></h2> <p>Isfiske på sjøen gir deg mulighet til å komme i kontakt med mange fiskeslag.</p> <p>Sjøørret, torsk, sei og lyr i de øvre vannlagene - og nær bunnen finner man knurr, hvitting, flydrer og andre både flate og avlange raringer.</p> <p> </p> <h2>Hvordan isfiske på sjøen</h2> <p><a title=\"Agnfisk - fiskebit\" href=\"index.php?option=com_content&view=article&catid=101:agn-og-kroker&id=46396:fiskebit\">Agnfisk</a>, <a title=\"Brisling\" href=\"index.php?option=com_content&view=article&catid=101:agn-og-kroker&id=46394:brisling\">brilsing</a>, <a title=\"Balansepilk\" href=\"index.php?option=com_content&view=article&catid=101:agn-og-kroker&id=46392:balansepilk\">balansepilk</a>, <a title=\"Pimpel\" href=\"index.php?option=com_content&view=article&catid=101:agn-og-kroker&id=46412:pimpel\">pimpel</a>, <a title=\"Gummimakk\" href=\"index.php?option=com_content&view=article&catid=101:agn-og-kroker&id=46398:gummimakk\">gummimakk</a>. Mulighetene er mange.</p> <p>Du må tenke litt på hvilket fiskeslag du vil ha, og tilpasse utstyret etter det. Men lett fortalt kan agnfisk på bunnen settes ut, mens man tar balansepilken i de øvre lagene.</p> <p>Er man på jakt etter sjøørret og annen stor hvitfisk, er det en fordel også å sette agnfisk litt høyt, og ha på nappvarsler.</p> <p>Agn og kroker du kan lese mer om;</p> <p> </p><ul> <li><a title=\"Agnfisk - Fiskebit\" href=\"index.php?option=com_content&view=article&catid=101:agn-og-kroker&id=46396:fiskebit\">Agnfisk</a> </li> <li><a title=\"Pimpel\" href=\"index.php?option=com_content&view=article&catid=101:agn-og-kroker&id=46412:pimpel\">Pimpel</a></li> <li><a title=\"Balansepilk\" href=\"index.php?option=com_content&view=article&catid=101:agn-og-kroker&id=46392:balansepilk\">Balansepilk</a></li> <li><a title=\"Brisling\" href=\"index.php?option=com_content&view=article&catid=101:agn-og-kroker&id=46394:brisling\">Brisling</a></li> </ul>  <p> </p> <p>Fisk deg oppover i vannlagene, men vinterstid står mange av de aktive fiskene ved bunn. Har du ikke kjent noe på noen minutter, kan du med fordel flytte deg til et nytt hull.</p> <p> </p> <h2>Utstyr for å isfiske på sjøen</h2> <ul> <li>Pimpel </li> <li>Lyskroker</li> <li>Vanlig små kroker</li> <li>Maggot</li> <li>Pilkestikke(r)</li> </ul> <p> </p> <p><strong>Annet isfiskeutstyr</strong></p> <ul> <li>Vanlig isfiskesnøre til pilkestikka. (Tåler kulde bedre enn vanlige snører)</li> <li>Isbor med litt dimensjon (en 4-5 kg torsk har stort og bredt hodet)</li> <li>Isøse</li> <li>Sittestol / liggeunderlag / reinsdyrsskinn</li> <li>Varmt drikke</li> <li>Varme klær og sokker (ull hele veien)</li> <li>Ispigger</li> <li>Flytedress anbefales</li> <li>Kasteline</li> </ul> <p> </p> <p>Skitt fiske!</p>', '', '1', '14', '0', '48', '2010-11-18 08:10:00', '103', '', '2011-04-22 09:55:00', '103', '0', '0000-00-00 00:00:00', '2010-11-18 08:10:00', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '4', '0', '5', 'isfiske på sjøen', 'Isfiske på sjøen er gøy, men vær aktsom på sjøisen\n\nIsfiske på sjøen gir deg mulighet til å komme i kontakt med mange fiskeslag. \nSjøørret, torsk, sei og lyr i de øvre vannlagene - og nær bunnen finner man knurr, hvitting, flydrer og andre både flate og avlange raringer.', '0', '216', 'robots=\nauthor='), ('8', 'En-hånds (One handed flyfishing)', 'en-hands-fluefisking', '', '&nbsp;', '', '-2', '14', '0', '79', '2010-11-18 08:11:00', '103', '', '2011-01-24 23:48:19', '62', '0', '0000-00-00 00:00:00', '2010-11-18 08:11:00', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '4', '0', '0', '', '', '0', '13', 'robots=\nauthor='), ('58480', 'Centroptilum luteolum', 'centroptilum-luteolum', '', '<h2><em>Centroptilum luteolum</em> er en Baetidae og ganske lik små <em>Baetis</em>-arter.</h2>\r\n<p>Enkelte steder er det omfattende <em>Centroptilum luteolum</em> klekkinger og kan gi godt fiske i fra juli og utover høsten.</p>\r\n', '\r\n<p> </p>\r\n<p> </p>\r\n<p><img style=\"border: 1px solid black;\" src=\"images/stories/insectpedia/centroptilum%20luteolum.jpg\" alt=\"\" /></p>\r\n<p>Foto: Kjetil Bergseth - Baethis Media</p>\r\n<p> </p>\r\n<p><strong>Imitajoner </strong></p>\r\n<p>Størrelse: 18-22</p>\r\n<p>Nymfer, klekker og spent spinner. Klekkere er å anbefale.</p>\r\n<p>Små baetisimitasjoner funker bra.</p>\r\n<p>Se bildegalleri i bunn for imitasjoner som passer.</p>\r\n<p> </p>\r\n<p><strong>Utbredelse </strong></p>\r\n<p>Finnes stort sett hele landet, meget utbredt på Østlandet og i Nord-Norge.</p>\r\n<p> </p>\r\n<p><strong>Utseende</strong></p>\r\n<p>Fra mørk oliven til lys, to haletråder. Sporeformet bakvinge. Krokstørrelse 18-22.</p>\r\n<p> </p>\r\n<p><strong>Klekkemønster </strong></p>\r\n<p>Klekker hovedsaklig fra midten av juli til utpå høsten. 2. generasjon klekking er meget liten (#22).</p>\r\n<p>Hovedsaklig nymfer og klekkere som er bra imitasjoner under fint vær. Duns sitter meget kort på vannfilmen, mens under regnbyger kan duns funke like bra.</p>', '1', '25', '0', '118', '2011-07-12 13:46:00', '103', '', '2012-01-02 22:31:12', '103', '0', '0000-00-00 00:00:00', '2011-07-12 13:46:00', '0000-00-00 00:00:00', 'jreviews/58480_Centroptilumluteolum_1323953350.jpg|||0||bottom||', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '3', '0', '1', 'Centroptilum luteolum', 'Centroptilum luteolum er en Baetidae og ganske lik små Baetis-arter.\r\n\r\nEnkelte steder er det omfattende Centroptilum luteolum klekkinger og kan gi godt fiske i fra juli og utover høsten. ', '0', '91', 'robots=\nauthor='), ('58481', 'Siphlonurus lacustris', 'siphlonurus-lacustris', '', '<p>Siphlonurus lacustris<br />tekst kommer</p>', '', '-2', '25', '0', '118', '2011-07-12 13:48:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-07-12 13:48:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '0', '', 'Siphlonurus lacustris', '0', '52', ''), ('58482', 'Siphlonurus', 'siphlonurus', '', '<h2><em>Siphlonurus</em> slekten har 3 arter i Norge.</h2>\r\n<p>Av de to mest aktuelle er<em> Siphlonurus lacustris</em> av snitt størrelse,  mens <em>aestivalis</em> kan by på en real munnfull. (20 mm)</p>\r\n', '\r\n<p><br /><img style=\"border: 1px solid black;\" src=\"images/stories/insectpedia/siphlonurus.jpg\" alt=\"\" /><br /> <br /><br />Foto: Kjetil Bergseth - Baethis Media</p>\r\n<p> </p>\r\n<p><br /><strong>Imitajoner</strong><br /><br />Størrelse: 14-16 eller 10-12.</p>\r\n<p>Denne raringen klekker inne ved steiner og vegetasjon, derfor er den mest aktuell å imitere som <strong>nymfe</strong> og <strong>spent spinner</strong>.</p>\r\n<p>Er det derimot litt vind, kan ferdig klekkede døgnfluer (duns) bli tatt av vinden ut i vannmasssende.<br /><br />Se bildegalleri i bunn for imitasjoner som passer.<br /><br /> <br /><br /><strong>Utbredelse</strong><br /><br />Finnes stort sett hele landet.<br /><br /> <br /><br /><strong>Utseende</strong><br /><br />Slekta er mørk  til lys, to haletråder. <br /><br /> <br /><br /><strong>Klekkemønster</strong><br /><br />Klekker hovedsaklig fra midten av juli til utpå høsten.<br /><br />Hovedsaklig nymfer og spent spinner som er bra imitasjoner.</p>', '1', '25', '0', '118', '2011-07-12 13:50:00', '103', '', '2012-01-02 22:32:54', '103', '0', '0000-00-00 00:00:00', '2011-07-12 13:50:00', '0000-00-00 00:00:00', 'jreviews/58482_Siphlonurus_1323977924.jpg|||0||bottom||', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '5', '0', '16', '', 'Siphlonurus\r\n\r\nSiphlonurus slekten har 3 arter i Norge. Av de to mest aktuelle er Siphlonurus lacustris av snitt størrelse,  mens aestivalis kan by på en real munnfull. (20 mm)', '0', '81', 'robots=\nauthor='), ('58483', 'Baetis Niger', 'baetis-niger', '', '<p>Baetis Niger<br />tekst kommer</p>', '', '0', '25', '0', '118', '2011-07-12 13:52:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-07-12 13:52:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '15', '', 'Baetis Niger', '0', '27', ''), ('58484', 'Baetis Mucticus', 'baetis-mucticus', '', '<p>Baetis Mucticus<br />tekst kommer</p>', '', '0', '25', '0', '118', '2011-07-12 13:53:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-07-12 13:53:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '14', '', 'Baetis Mucticus', '0', '28', ''), ('58485', 'Heptagenia sulphurea', 'heptagenia-sulphurea', '', '<p><strong>Heptagenia sulphurea, også kalt \"gul flatdøgnflue\" er karakteristisk i sin knall gule farge.</strong>  </p>\r\n', '\r\n<p> </p>\r\n<p><img style=\"border: 1px solid black;\" src=\"images/stories/insectpedia/sulphurea.jpg\" alt=\"\" /></p>\r\n<p>Foto: Kjetil Bergseth - Baethis Media</p>\r\n<p> </p>\r\n<p> </p>\r\n<p><strong>Imitasjoner</strong></p>\r\n<p>Vanlige nymfer, klekkere og duns.<span style=\"text-decoration: underline;\"><strong> i gult.</strong></span></p>\r\n<p>Se bildegalleri i bunn for imitasjoner som passer.</p>\r\n<p> </p>\r\n<p><strong>Utbredelse</strong></p>\r\n<p>Finnes; Østlandet • Midt-norge • Vestlandet • Sørlandet</p>\r\n<p> </p>\r\n<p><strong>Utseende</strong></p>\r\n<p>Kroppen er 10-14 millimeter lang.2 haletråder. Gul, meget lett gjennkjennelig gule årer i fremkanten av vingene. </p>\r\n<p> </p>\r\n<p><strong>Levevis</strong></p>\r\n<p>Gul flatdøgnflue finnes i bekker og større elver med rennende vann. </p>\r\n<p> </p>\r\n<p><strong>Klekkemønster</strong></p>\r\n<p>Klekker ute i de fri vannmassene og gir gode tørrflueforhold. Klekker jevnt og trutt over lang tid, ikke forvent teppeklekking.</p>', '1', '25', '0', '118', '2011-12-31 23:00:00', '103', '', '2012-01-01 21:12:36', '103', '0', '0000-00-00 00:00:00', '2011-12-31 23:00:00', '0000-00-00 00:00:00', 'jreviews/58485_Sulphurea_1325449850.jpg|||0||bottom||', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '7', '0', '13', '', 'Heptagenia sulphurea', '0', '127', 'robots=\nauthor='), ('58486', 'Heptagenia fuscogrisea', 'heptagenia-fuscogrisea', '', '<h2><strong><em>Heptagenia fuscogrisea</em>, kalt Brun flatdøgnflue, er en døgnflue som klekker tidlig på forsommeren.</strong></h2>\r\n', '\r\n<p> </p>\r\n<p><img style=\"border: 1px solid black;\" src=\"images/stories/insectpedia/fuscogrisea.jpg\" alt=\"\" /></p>\r\n<p>Foto: Kjetil Bergseth - Baethis Media</p>\r\n<p> </p>\r\n<p> </p>\r\n<p><strong>Imitajoner</strong></p>\r\n<p><strong>Dun</strong>; Parachute</p>\r\n<p><strong>Nymfe</strong> og <strong>klekker</strong></p>\r\n<p>Se bildegalleri i bunn for imitasjoner som passer.</p>\r\n<p>  </p>\r\n<p><strong>Utbredelse</strong></p>\r\n<p>Finnes i hele landet med unntak av vestlandet.</p>\r\n<p>  </p>\r\n<p><strong>Utseende</strong></p>\r\n<p>En nyklekket dun er 10-14mm lang har 2 haletråder og er brun.</p>\r\n<p> </p>\r\n<p><strong>Klekkemønster</strong></p>\r\n<p>Finnes i både stille- og rennende vann. Liker stenbunn. Klekker tidlig på forsommeren, en tre-fire uker etter isen har gått.</p>', '1', '25', '0', '118', '2011-07-12 13:57:00', '103', '', '2012-01-02 22:15:14', '103', '0', '0000-00-00 00:00:00', '2011-07-12 13:57:00', '0000-00-00 00:00:00', 'jreviews/58486_Fuscogrisea_1325542455.jpg|||0||bottom||', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '9', '0', '12', '', 'Heptagenia fuscogrisea', '0', '122', 'robots=\nauthor='), ('58487', 'Heptagenia dalecarlica', 'heptagenia-dalecarlica', '', '<p>Heptagenia dalecarlica<br />tekst kommer</p>', '', '0', '25', '0', '118', '2011-07-12 13:59:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-07-12 13:59:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '11', '', 'Heptagenia dalecarlica', '0', '26', ''), ('58488', 'Caenis', 'caenis', '', '<p>Caenis<br />tekst kommer</p>', '', '0', '25', '0', '118', '2011-07-12 14:01:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-07-12 14:01:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '10', '', 'Caenis', '0', '26', ''), ('58489', 'Vårflue', 'varflue', '', '<h2><strong>Vårflua er lett å identifisere med de hårete vingene som ligger skrått over bakkroppen. </strong></h2>\r\n', '\r\n<p> </p>\r\n<p>Men ikke la deg lure av navnet. \"Vår\" har fint lite med dens levemønster å gjøre. Faktisk er vårflua den siste av de fire viktige insektene som klekker om forsommeren. \"Caddis\", dets engelske navn, drar du kanskje kjensel på.</p>\r\n<p> </p>\r\n<p><img style=\"border: 1px solid black;\" src=\"images/stories/insectpedia/vrflue.jpg\" alt=\"\" /></p>\r\n<p>Foto: Kjetil Bergseth - Baethis Media</p>\r\n<p> </p>\r\n<p><strong>Vårflua er et meget viktig insekt</strong> mange steder i landet, og kan spille en større rolle en døgnfluer store deler av fluefiskesesongen.</p>\r\n<p>Selv om finnes flere vårfluefamilier (18) og arter (195), og dermed er langt flere enn døgnfluene (46), er det de færreste fluefiskerne om har tatt seg bryet med å lære å identifisere vårfluene. Det greier seg med en håndfull imitasjoner i boksen for de aller fleste.</p>\r\n<p> </p>\r\n<p><strong>Imitajoner</strong></p>\r\n<p><strong>Larver</strong>: Både med og uten hus.</p>\r\n<p><strong>Pupper</strong>: #12 - 16. Super puppan, Sparkle yarn, Standard puppa, F-Fly m.fl.</p>\r\n<p><strong>Tørrflue / vokse individer:</strong> #12-16. March Brown, Rackelhanan, Streaking Caddis m.fl.</p>\r\n<p>Både størrre og mindre individer finnes. Her må du nesten se hva som klekker der du er.</p>\r\n<p><br />Se bildegalleri i bunn for imitasjoner som passer.<br /><br /><br /><br /><strong>Utbredelse</strong><br />Finnes i hele Norge, overalt hvor det er ferskvann.</p>\r\n<p><br /><br /><strong>Utseende</strong><br />Store vinger som ligger over bakkroppen. Mange mener bestemt det likner på et \"telt\".<br /><br /><br /><br /><strong>Klekkemønster</strong><br />Vårfluene kan klekke i enorme mengder. Er \"stressa\" og lager mye liv på vannet. Enkelte ganger bølger det voldsomt.</p>\r\n<p>Ulike størrelser gjør at det kan bli kilent å knekke koden. Kan selvsagt være lurt å studere hva ørreten faktisk tar, med å fange et par eksemplarer. Noen arter klekker på kvelden, andre om dagen. Enkelte arter blir tiltrukket av lys, og de kan derfor fly til lyskilder når det er mørkt.</p>', '1', '25', '0', '119', '2012-01-08 23:00:00', '103', '', '2012-01-09 21:14:44', '103', '0', '0000-00-00 00:00:00', '2012-01-08 23:00:00', '0000-00-00 00:00:00', 'jreviews/58489_Vrflue_1326142632.jpg|||0||bottom||', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '7', '0', '1', '', 'Vårflue', '0', '155', 'robots=\nauthor='), ('58490', 'Fjærmygg', 'fjaermygg', '', '<h2><strong>Fjærmygg finnes i hele landet og er et av de aller viktigste byttedyrene for fisken.</strong></h2>\r\n', '\r\n<p> </p>\r\n<p>Mange ganger kan fjærmygg være det viktigste insektet å imitere, spesielt for fluefiskere som bruker mye tid i innsjøer og tjern.</p>\r\n<p> </p>\r\n<p><img style=\"border: 1px solid black;\" src=\"images/stories/insectpedia/fjrmygg.jpg\" alt=\"\" /></p>\r\n<p>Foto: Kjetil Bergseth - Baethis Media</p>\r\n<p> </p>\r\n<p><strong>Imitajoner</strong><br />Pupper: #12 - 28</p>\r\n<p>Tørrflue / vokse individer: #14-24</p>\r\n<p><br /><strong>Spesielt pupper</strong> (les: klekkere) for å fiske i vannfilmen.</p>\r\n<p><strong>Tørrflue</strong> (les: utviklede fjærmygger): Palmermygg / Griffith\'s Gnat, No-Hackle mygg, Fallskjermmygg og Spent mygg.<br /><br />Se bildegalleri i bunn for imitasjoner som passer.<br /><br /><br /><br /><strong>Utbredelse</strong><br />Finnes i hele Norge, overalt hvor det er ferskvann. Også i stilleflytende partier i elver.</p>\r\n<p>Kan klekke hele året, bare det er åpent vann.<br /><br /><br /><br /><strong>Utseende</strong><br />Voksne invivider er små og mørke. Larvene kan være både hvite, grå eller rød - avhengig av slekt og habitat.<br /><br /><br /><br /><strong>Klekkemønster</strong><br />Heldigvis klekker fjærmyggen ute i vannmassene, og ikke inne blandt steiner og kratt i vannkanten. Noe som bidrar til at det kan gi gode tørrflueforhold.</p>\r\n<p>Den får ekstra hjelp til å bryte vannfilmen hvis det er litt ruskevær eller vind, er det speilblankt bruker den lang tid på å bryte vannfilmen.</p>\r\n<p>Vaking på kvelden kommer ofte av \"ryddefisk\" som beiter på pupper i eller rett under overflaten.</p>', '1', '25', '0', '120', '2012-01-05 21:00:00', '103', '', '2012-01-05 22:01:28', '103', '0', '0000-00-00 00:00:00', '2012-01-05 21:00:00', '0000-00-00 00:00:00', 'jreviews/58490_Fjrmygg_1325799435.jpg|||0||bottom||', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink_section=\nshow_category=\nlink_category=\nshow_vote=\nshow_author=\nshow_create_date=\nshow_modify_date=\nshow_pdf_icon=\nshow_print_icon=\nshow_email_icon=\nlanguage=\nkeyref=\nreadmore=', '7', '0', '1', '', 'Fjærmygg finnes i hele landet og er et av de aller viktigste byttedyrene for fisken.', '0', '91', 'robots=\nauthor='), ('58491', 'Stankelbein', 'stankelbein', '', '<p>Stankelbein<br />tekst kommer</p>', '', '0', '25', '0', '120', '2011-07-12 14:08:00', '103', '', '0000-00-00 00:00:00', '0', '0', '0000-00-00 00:00:00', '2011-07-12 14:08:00', '0000-00-00 00:00:00', '', '', '', '1', '0', '2', '', 'Stankelbein', '0', '31', ''), ('58492', 'Steinflue', 'steinflue', '', '<p>Steinflue<br />tekst kommer</p>', '', '0', '25', '0', '119', '2011-07-12 14:10:00', '103', '', '2011-07-12 14:26:38', '103', '0', '0000-00-00 00:00:00', '2011-07-12 14:10:00', '0000-00-00 00:00:00', '', '', 'show_title=\nlink_titles=\nshow_intro=\nshow_section=\nlink